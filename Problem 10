const isPrime = (n) => {
	let ceil = Math.ceil(Math.sqrt(n));
	for (let i = 2; i <= Math.ceil(Math.sqrt(n)); i++) {
		if (n % i == 0) return false;
	}
	return true;
};

const primeSummation = (n) => {
	const arr = [2, 3];
	for (let i = 5; i < n; i += 2) {
		if (isPrime(i)) arr.push(i);
	}
	return arr.reduce((acc, curr) => acc + curr);
};
