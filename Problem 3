function largestPrimeFactor(number) {
  function isPrime(n) {
    for(let i = 2; i < n; i++) {
      if(n % i == 0) return false;
    }
    return true;
  }
  const arr = [];
  for(let i = 2; i <= number; i++) {
    if(isPrime(i) && number % i == 0) arr.push(i)
  }

  return arr[arr.length - 1];
}

//this works but very resource intensize
