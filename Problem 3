const largestPrimeFactor = (number) => {  
  const isPrime = (n) => {
    for(let i = 2; i < n; i++) {
      if(n % i == 0) return false;
    }
    return true;
  }
  let factor = number;
  const arr = [];
  for(let i = 2; i <= factor; i++) {
    if(isPrime(i) && factor % i == 0) {
      arr.push(i);
      factor = factor/i;
    }
  }

  return arr[arr.length - 1];
}
