const largestPrimeFactor = (number) => {  
  const isPrime = (n) => {
    for(let i = 2; i < n; i++) {
      if(n % i == 0) return false;
    }
    return true;
  }
  const arr = [];
  for(let i = 2; i <= number; i++) {
    if(isPrime(i) && number % i == 0) {
      arr.push(i);
      number = number/i;
    }
  }

  return arr[arr.length - 1];
}
